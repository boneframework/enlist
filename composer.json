{
    "name": "boneframework/enlist",
    "description": "User role registration using delboy1978uk/passport",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
        "delboy1978uk/bone-doctrine": "^2.3",
        "delboy1978uk/passport": "^1.2",
        "delboy1978uk/user": "^4.9"
    },
    "require-dev":{
        "delboy1978uk/dev-tools": "^1.0.0"
    },
    "autoload":{
        "psr-4":{
            "Bone\\Passport\\Enlist\\":"src/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "Bone\\Passport\\Enlist\\Tests\\": "tests/unit"
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": false
        }
    }
}
